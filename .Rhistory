getMap <- function(n,iter,configFile){
#### SETTING PATHS
## check machine
## set working directory
basePath <- r"(OneDrive - Drexel University/bbland/bbland-github)"
setwd(basePath)
## changes each iteration
nPath <- file.path("data",file.path(paste("n",n,sep = "")))
nPath <- file.path(basePath,nPath)
iterPath <- file.path(nPath,paste("iter",iter,sep = ""))
ifelse(!dir.exists(nPath), dir.create(nPath), FALSE)
ifelse(!dir.exists(iterPath), dir.create(iterPath), FALSE)
### LOADING FUNCTIONS
source("functions.R")
contASCFile <- file.path(nPath,paste("cont",n,".asc",sep = ""))
if (!file.exists(contASCFile)){
print("here")
rr_cont<- getLandscape(n,NULL)
# contASCFile <- file.path(nPath,paste("cont",n,".asc",sep = ""))
writeRaster(rr_cont, contASCFile, format="ascii", overwrite = T)
convertASCtoPTC(nPath, nPath,"cont",n,NULL)
getContBatchFile(n,nPath)
}
## map of "preserved" areas
configFile <- file.path(iterPath,configFile)
s<- read.csv(configFile,header = FALSE)
configuration<- as.numeric(t(s))
rr_bin<- getLandscape(n,configuration)
writeRaster(rr_bin, file.path(iterPath,paste("binary",n,"_",iter,".asc",sep = "")), format="ascii", overwrite = T)
convertASCtoPTC(iterPath,iterPath,"binary",n,iter)
convertPTCtoPDY(nPath, iterPath,iterPath,n,iter)
getBatchFile(n,iter,iterPath,iterPath)
}
